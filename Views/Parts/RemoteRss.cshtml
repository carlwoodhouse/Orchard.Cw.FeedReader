@{
    var feed = ((IEnumerable<dynamic>) Model.Feed.Element("channel").Elements("item"));
}
<ul>
@{
    var count = 1;
    
    foreach(var item in feed) {
        <li><a href="@item.Element("link").Value">@item.Element("title").Value</a></li>
        if (count == (int)Model.ItemsToDisplay ) {
            break;
        }

        count++;
    }
}
</ul>